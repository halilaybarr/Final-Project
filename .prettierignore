package-lock.json
yarn.lock

# Ignore log files
*.log

# Ignore environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore Vite build output
dist/

# Ignore cache directories
.cache/
.parcel-cache/

# Ignore minified files
*.min.js
*.min.css

# Ignore generated files
*.generated.*
